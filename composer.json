{
    "name": "melisplatform/melis-composerdeploy",
    "description": "Melis Platform Composer deployment module",
    "type": "melisplatform-module",
    "license": "OSL-3.0",
    "homepage": "https://github.com/melisplatform/melis-composerdeploy",
    "support": {
        "source": "https://github.com/melisplatform/melis-composerdeploy",
        "issues": "https://github.com/melisplatform/melis-composerdeploy/issues"
    },
    "keywords": [
        "cms",
        "melis",
        "zf2",
        "module"
    ],
    "extra": {
        "module-name": "MelisComposerDeploy",
        "dbdeploy": true
    },
    "require": {
        "php": ">=5.4",
        "composer/composer": "~1.2",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "php-mock/php-mock": "~2.0.0",
        "psy/psysh": "@stable",
        "symfony/console": "~3.4|~4.0"
    },
    "autoload": {
        "psr-4": {
            "MelisComposerDeploy\\": "src/"
        }
    }
}
